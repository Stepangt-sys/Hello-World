using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CreateObject : MonoBehaviour
{
    // Start is called before the first frame update
    void Start()
    {
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(0, 0, 0);
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(-0,5f, 1, 0);
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(0,5f, 1, 0);
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(0,5f, 2, 0);
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(-0,5f, 2, 0);
        GameObjects.CreatePrimitive(PrimitiveType.Cube).position = new Vector3(0, 3, 0);
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
